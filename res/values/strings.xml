        <item >Videos</item>
        <item >Photos</item>
        <item >Audios</item>
    </string-array>
     
    <!-- Nav Drawer List Item Icons entered in the same order -->

    <array name="nav_drawer_icons">
        <item>@drawable/ic_action_video</item>
        <item>@drawable/ic_action_picture</item>   
        <item>@drawable/audio</item>     
    </array>
  
</resources>


