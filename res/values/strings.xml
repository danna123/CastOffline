<resources>
    <string name="app_name">CastOffline</string>
    <string name="action_settings">Settings</string>
    <string name="open">Slider Menu Opened</string>
    <string name="close">Slider Menu Closed</string>
    
    <!-- Enter the app id . 
    Refer : https://github.com/danna123/CastOffline/blob/master/Documents/Installationguidefordevelopers.pdf-->
    <string name="app_id">ENTER YOUR APP ID OR DEFAULT APP ID</string>
    
